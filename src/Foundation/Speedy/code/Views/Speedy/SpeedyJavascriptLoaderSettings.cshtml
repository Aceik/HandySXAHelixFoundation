@using Sitecore.Foundation.Speedy
@using Sitecore.Foundation.Speedy.Settings
@using Sitecore.XA.Foundation.Theming.Bundler

Speedy.settings = {
    cookieName: '@Html.Raw(SpeedyConstants.CookieNames.OnePassCookieName)',
