    jsDeferLoadForMilliseconds: @Html.Raw(SpeedyGenerationSettings.GetDeferJSLoadForMilliseconds()),
    fallBackExperienceDelay: @Html.Raw(SpeedyGenerationSettings.GetDeferFallbackMilliseconds()),
    stylesDeferMilliseconds: @Html.Raw(SpeedyGenerationSettings.GetDeferCSSLoadForMilliseconds()),
    fallbackHideSelector : '@Html.Raw(SpeedyGenerationSettings.GetFallbackExperienceSelector())'
};
